[user]
	name = {{ (awsSecretsManager "GITHUB_USER_INFO").username }}
	email = {{ (awsSecretsManager "GITHUB_USER_INFO").email }}
[init]
	defaultBranch = main
[core]
	editor = vim -c \"set fenc=utf-8\"
[alias]
	tree = log --graph --pretty=format:'%x09%C(auto) %h %Cgreen %ar %Creset%x09by"%C(cyan ul)%an%Creset" %x09%C(auto)%s %d'
