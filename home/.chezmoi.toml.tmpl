# chezmoi configuration file
# This file defines settings and variables used across dotfiles

[data]
    # GitHub configuration
    [data.github]
        username = "ysd-rysn"
        repository = "dotfiles"

    # Script paths
    [data.paths]
        scripts_dir = "{{ .chezmoi.workingTree }}/.scripts"
        ssh_dir = "{{ .chezmoi.homeDir }}/.ssh"

[awsSecretsManager]
    profile = "chezmoi"
    region = "ap-northeast-1"
